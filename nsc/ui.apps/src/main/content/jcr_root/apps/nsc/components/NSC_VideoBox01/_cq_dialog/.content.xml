<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="NSC_VideoBox01"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[nsc.clientlibs.NSC_accodion]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <mainTab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Main Tab"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <mainItems
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                granite:class="accordion-count"
                                                fieldLabel="Main Items">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        min-items="2"
                                                        max-items="5"/>
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                                    name="./mainItems">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <mainItem
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="Sub Items"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <mainTitle
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Main Title"
                                                                    name="./mainTitle"/>
                                                            </items>
                                                        </mainItem>
                                                    </items>
                                                </field>
                                            </mainItems>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </mainTab>
                    <tab1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <videoTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="title"
                                                name="./videoTitle"/>
                                            <videoComment
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="comment"
                                                name="./videoComment"/>
                                            <videoImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="video Image"
                                                name="./videoImage"
                                                rootPath="/content/dam"/>
                                            <videoUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="video"
                                                name="./videoUrl"
                                                rootPath="/content/dam"/>
                                            <videoAlert
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Video Alert"
                                                name="./videoAlert"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </tab1>
                    <tab2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <worksItems
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                granite:class="accordion-count"
                                                fieldLabel="Work Items">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        min-items="2"
                                                        max-items="12"/>
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/accordion"

                                                    name="./worksItems">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <worksItem
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="Sub Items"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <workYear
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Work Year"
                                                                    name="./workYear"/>
                                                                <workName
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Work Name"
                                                                    name="./workName"/>
                                                                <workComponey
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Work Componey"
                                                                    name="./workComponey"/>
                                                                <workComment
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                                    fieldLabel="Work Comment"
                                                                    name="./workComment"/>
                                                            </items>
                                                        </worksItem>
                                                    </items>
                                                </field>
                                            </worksItems>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </tab2>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
